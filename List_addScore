grades = [[45,85,60],[100,98,95],[85,91,90]]

print(grades)
for row_i, row in enumerate(grades):
    
    #print(row)
    for i, col in enumerate(row):
        
        print('Before', col)
        
        # add 5 to that value
        grades[row_i][i] += 5
        print(col)
        
    print()

print(grades)
